<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<requestforquotepdf>
			<general>
				<pdf_footer>Landofcoder.com | Request For Quote</pdf_footer>
				<store_logo>default/logo.jpg</store_logo>
				<enable_quote_info>1</enable_quote_info>
				<enable_billing>1</enable_billing>
				<enable_shipping>1</enable_shipping>
				<enable_other_info>1</enable_other_info>
				<enable_image>1</enable_image>
			</general>
		</requestforquotepdf>
	</default>
</config>